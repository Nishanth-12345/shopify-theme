img{
    width: 100px;
    height: 60px;
}

body{
    padding: 0;
    margin: 0;
}

.announcement-bar{
    display: flex;
    justify-content: space-between;
    padding:0 30px;
}

.announcement-bar .left-section{
    display: flex;
    align-items: center;
}

.announcement-bar .announcement-bar-inner-list{
    display: flex;
    align-items: center;
}

.announcement-bar .announcement-bar-inner-list li{
    padding: 2px;
    font-size: 14px;
    list-style-type: none;
    text-decoration: none;
}

.announcement-bar .announcement-bar-inner-list li a{
    color: inherit;
    text-decoration: none;
}


